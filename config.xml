<?xml version='1.0' encoding='utf-8'?>
<widget id="com.glenn1.hello" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>glenn's app</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="Fullscreen" value="false" />
    <plugin name="cordova-plugin-nativestorage" spec="^2.3.1" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <icon src="untitled.png" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <splash density="land-hdpi" src="untitled.png" />
    <splash density="port-hdpi" src="untitled.png" />
    <key>UIStatusBarHidden</key>
    <true />
    <key>UIViewControllerBasedStatusBarAppearance</key>
    <false />
    <preference name="android-windowSoftInputMode" value="stateVisible" />
    <engine name="android" spec="^7.0.0" />
    <engine name="browser" spec="^5.0.3" />
</widget>
